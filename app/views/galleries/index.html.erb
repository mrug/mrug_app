<h1>Listing galleries</h1>

<table>
  <thead>
    <tr>
      <th>Photo file name</th>
      <th>Photo file size</th>
      <th>Photo content type</th>
      <th>Photo updated at</th>
      <th>Event</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @galleries.each do |gallery| %>
      <tr>
        <td><%= gallery.photo_file_name %></td>
        <td><%= gallery.photo_file_size %></td>
        <td><%= gallery.photo_content_type %></td>
        <td><%= gallery.photo_updated_at %></td>
        <td><%= gallery.event_id %></td>
        <td><%= link_to 'Show', gallery %></td>
        <td><%= link_to 'Edit', edit_gallery_path(gallery) %></td>
        <td><%= link_to 'Destroy', gallery, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Gallery', new_gallery_path %>
